{
  "name": "senv-inquirer",
  "version": "0.1.0",
  "description": "Inquirer prompt for senv variables",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean"
  },
  "peerDependencies": {
    "inquirer": "^8.0.0 || ^9.0.0",
    "senv": "workspace:*"
  },
  "devDependencies": {
    "@types/inquirer": "^8.2.0",
    "inquirer": "^9.2.0",
    "senv": "workspace:*",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  }
}
